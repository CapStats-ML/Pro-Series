# Cargar las bibliotecas necesarias
library(quantmod)
library(rugarch)
# Descargar datos de ejemplo (rendimientos de Apple Inc.)
getSymbols("AAPL", from = "2019-01-01", to = "2024-01-01")
returns <- diff(log(AAPL$AAPL.Close))
returns <- returns[-1]  # Eliminar la primera observación NA
# Función para calcular el Quantilograma
quantilogram <- function(x, q, k_max) {
n <- length(x)
I_t <- as.numeric(x <= quantile(x, q))
I_t_k <- I_t[(k_max+1):n]
rho <- numeric(k_max)
for (k in 1:k_max) {
I_t_lag <- I_t[((k_max+1-k):(n-k))]
rho[k] <- sum((I_t_k - q) * (I_t_lag - q)) / sum((I_t - q)^2)
}
return(rho)
}
# Calcular el Quantilograma para diferentes cuantiles
k_max <- 20
q_values <- c(0.1, 0.5, 0.9)
results <- lapply(q_values, function(q) quantilogram(returns, q, k_max))
# Crear un data frame para la visualización
df <- data.frame(
lag = rep(1:k_max, 3),
rho = unlist(results),
quantile = rep(q_values, each = k_max)
)
# Visualizar el Quantilograma
ggplot(df, aes(x = lag, y = rho, color = factor(quantile))) +
geom_line() +
geom_hline(yintercept = 0, linetype = "dashed") +
labs(title = "Quantilograma de los rendimientos de AAPL",
x = "Lag", y = "Autocorrelación", color = "Cuantil") +
# Visualizar el Quantilograma
ggplot(df, aes(x = lag, y = rho, color = factor(quantile))) +
geom_line() +
geom_hline(yintercept = 0, linetype = "dashed") +
labs(title = "Quantilograma de los rendimientos de AAPL",
x = "Lag", y = "Autocorrelacion", color = "Cuantil") +
theme_minimal()
